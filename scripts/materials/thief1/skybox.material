// ---------------- Standard Thief 1 skyboxes ----------------------

//----- daylight skybox 
material Skybox/daysky
{
	technique 
	{
		pass 
		{
			ambient 0.0 0.0 0.0 0.0
			diffuse 0.0 0.0 0.0 0.0
			specular 0.0 0.0 0.0 0.0

			scene_blend alpha_blend	
		}
	}
}

//----- night skybox
material Skybox/nightsky
{
	technique 
	{
		pass 
		{
			ambient 0.0 0.0 0.0 0.0
			diffuse 0.0 0.0 0.0 0.0
			specular 0.0 0.0 0.0 0.0

			scene_blend alpha_blend	
		}
	}
}
